{
  "manifest_version": 3,
  "name": "Live Student Feedback",
  "description": "Live feedback between students and teachers",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "host_permissions": [
    "https://docs.google.com/*"
  ]
}